
@model ListedIN.Models.ApplicationUser


<div class="row" id="myProfile_Sec1">
    <div class="col-md-8">

        <button type="button" class="btn btn-link" style="float: right" data-toggle="modal" data-target="#myModal">
            Edit
        </button>

        <div class="thumbnail">
       
            <div class="caption">



                <h3> @Model.FirstName @Model.LastName </h3>
                                                
                
                @if (Model.Position != null)
                {

                    <h4>@Model.Position</h4>

                }

                @if (Model.HeadLine != null)
                {

                    <h5>@Model.HeadLine</h5>

                }
                      
              

                @if (Model.Country != 0)
                {

                    <p>@Model.Country</p>

                }

              
              

                @if (Model.Summary != null)
                {

                    <p>@Model.Summary</p>

                }
            </div>
        </div>
    </div>
</div>

